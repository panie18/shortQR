{
  "name": "qr-short",
  "version": "1.0.0",
  "description": "QR Code Generator and URL Shortener for umbrelOS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js & npm run watch:css",
    "build": "npm run build:css",
    "build:css": "postcss src/css/tailwind.css -o public/styles.css --minify",
    "watch:css": "postcss src/css/tailwind.css -o public/styles.css --watch"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "nanoid": "^3.3.6",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3"
  },
  "author": "",
  "license": "MIT"
}